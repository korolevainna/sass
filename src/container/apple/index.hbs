<div class="page">
  <div class="header">
      {{#with header}}
      <header class="header__content">
        <div class="header__block">
        <img src="/svg/apple.svg" alt="Aplle" class="header__icon">

        <nav class="header__block">
          {{#each links}}
            <a href="url" class="header__links">{{text}}</a>
          {{/each}}
        </nav>
        <div class="header__action">
          <img src="/svg/search.svg" alt="Search" class="header__icon">
          <img src="/svg/profile.svg" alt="Profile" class="header__icon">
        </div>
        </div>
        <div class="header__ad">
          <div class="header__block-2">
          <p class="header__title">{{title}}</p>
          {{#each link}}
            <a href="url" class="header__link">{{text}}</a>
          {{/each}}
        </div>
        </div>
      </header>
      {{/with}}
    </div>

  {{#with iphone_pro}}
    <section class="product product-big product--iphone-pro product--dark product--top">
      <div class="product__content">
        <h1 class="product__title">{{title}}</h1>
        <p class="product_descriptions">{{descriptions}}</p>
        <div class="product__action">
          {{#with link}}
          {{#each text}}
            <a href="{{../link}}" class="product__linc">{{this}}</a>
          {{/each}}
          {{/with}}
        </div>
      </div>
    </section>
    {{/with}}

